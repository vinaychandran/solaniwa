    <div class="nv-lang">
        <div class="current-lang">
            <img src="/assets/images/cmn/lang-ja.png" alt="ja">
            <div class="nv-lang-others">
                <a href="/en/"><img src="/assets/images/cmn/lang-en.png?1552030578593" alt="en"></a>
            </div>
        </div>
